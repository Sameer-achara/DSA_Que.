// subtract-the-product-and-sum-of-digits-of-an-integer

class Solution {
public:
    int subtractProductAndSum(int n) {
        int sum=0;
        int pro=1;
    while(n>0){
        int rem = n%10;
        sum=sum+rem;
        pro=pro*rem;
        n=n/10;
    }
    int answer=(pro-sum);
    return answer;
    }
};
