#include <iostream>
using namespace std;
int main() {
    for(int i = 1; i <= 7; i++){
        for(int j = i + 1; j <= 7; j++){  
            for(int k = j + 1; k <= 7; k++){
                if((i+j+k)==12){
                cout << i << " " << j << " " << k << endl;
                }
            }
        }
    }
    return 0;
}
