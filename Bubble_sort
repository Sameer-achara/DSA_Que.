#include<iostream>
using namespace std;
void bubble(int arr[],int n){
    for(int i=0;i<n-1;i++){
        for(int j=0;j<n-i-1;j++){
            if(arr[j]>arr[j+1]){
                swap(arr[j],arr[j+1]);
            }
        }
    }  
}
int main(){
    int array[10]={7,9,10,4,2,6};
    bubble(array,6);
    cout<<"Sorted Array is:"<<endl;
    for(int i=0;i<6;i++){
    cout<<array[i]<<" ";
    }
    return 0;
}
